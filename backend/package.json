{
  "name": "pensiv-backend",
  "version": "1.0.0",
  "description": "Backend API for Pensiv - A modern, full-stack blogging platform",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnuragDahal/pensiv.git"
  },
  "keywords": [
    "blog",
    "cms",
    "content-management",
    "nodejs",
    "express",
    "mongodb",
    "typescript",
    "api"
  ],
  "author": "Anurag Dahal",
  "license": "MIT",
  "homepage": "https://github.com/AnuragDahal/pensiv#readme",
  "bugs": {
    "url": "https://github.com/AnuragDahal/pensiv/issues"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon",
    "build": "tsc && tsc-alias",
    "prod": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "seed": "ts-node -r tsconfig-paths/register scripts/seed.ts",
    "seed:full": "ts-node -r tsconfig-paths/register scripts/comprehensive-seed.ts",
    "generate-embeddings": "ts-node -r tsconfig-paths/register src/features/chat/generate-embeddings.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/marked": "^6.0.0",
    "@types/node": "^22.13.9",
    "@types/striptags": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.35.0"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fuse.js": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "marked": "^12.0.2",
    "mongoose": "^8.9.5",
    "striptags": "^3.2.0",
    "tsc-alias": "^1.8.16",
    "unsplash-js": "^7.0.20",
    "zod": "^3.24.1"
  },
  "pnpm": {
    "overrides": {
      "qs": ">=6.14.1"
    }
  }
}
